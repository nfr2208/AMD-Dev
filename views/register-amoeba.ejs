<%- include('includes/head.ejs')%>
</head>
<!-- end::Head -->
<!-- end::Body -->

<body
	class="m-page--fluid m--skin- m-content--skin-light2 m-header--fixed m-header--fixed-mobile m-aside-left--enabled m-aside-left--skin-light m-aside-left--fixed m-aside-left--offcanvas m-aside-left--minimize m-brand--minimize m-footer--push m-aside--offcanvas-default">
	<!-- begin:: Page -->
	<div class="m-grid m-grid--hor m-grid--root m-page">
		<!-- BEGIN: Header -->
		<%- include('includes/header.ejs')%>
		<!-- END: Header -->
		<!-- begin::Body -->
		<div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-body">
			<!-- BEGIN: Left Aside -->
			<%- include('includes/left-aside.ejs')%>
			<!-- END: Left Aside -->
			<div class="m-grid__item m-grid__item--fluid m-wrapper">
				<!-- BEGIN: Subheader -->
				<div class="m-subheader ">
					<div class="d-flex align-items-center">
						<div class="mr-auto">
							<h3 class="m-subheader__title m-subheader__title--separator">
								Dashboard
							</h3>
							<ul class="m-subheader__breadcrumbs m-nav m-nav--inline">
								<li class="m-nav__item m-nav__item--home">
									<a href="#" class="m-nav__link m-nav__link--icon">
										<i class="m-nav__link-icon la la-home"></i>
									</a>
								</li>
								<li class="m-nav__separator">
									>
								</li>
								<li class="m-nav__item">
									<a href="" class="m-nav__link">
										<span class="m-nav__link-text">
											Dashboard
										</span>
									</a>
								</li>
								<li class="m-nav__separator">
									>
								</li>
								<li class="m-nav__item">
									<a href="" class="m-nav__link">
										<span class="m-nav__link-text">
											Register Amoeba
										</span>
									</a>
								</li>
							</ul>
						</div>
						<div>
							<div class="m-dropdown m-dropdown--inline m-dropdown--arrow m-dropdown--align-right m-dropdown--align-push"
								m-dropdown-toggle="hover" aria-expanded="true">
								<a href="#"
									class="m-portlet__nav-link btn btn-lg btn-secondary  m-btn m-btn--outline-2x m-btn--air m-btn--icon m-btn--icon-only m-btn--pill  m-dropdown__toggle">
									<i class="la la-plus m--hide"></i>
									<i class="la la-ellipsis-h"></i>
								</a>
								<div class="m-dropdown__wrapper">
									<span
										class="m-dropdown__arrow m-dropdown__arrow--right m-dropdown__arrow--adjust"></span>
									<div class="m-dropdown__inner">
										<div class="m-dropdown__body">
											<div class="m-dropdown__content">
												<ul class="m-nav">
													<li class="m-nav__section m-nav__section--first m--hide">
														<span class="m-nav__section-text">
															Quick Actions
														</span>
													</li>
													<li class="m-nav__item">
														<a href="" class="m-nav__link">
															<i class="m-nav__link-icon flaticon-share"></i>
															<span class="m-nav__link-text">
																Activity
															</span>
														</a>
													</li>
													<li class="m-nav__item">
														<a href="" class="m-nav__link">
															<i class="m-nav__link-icon flaticon-chat-1"></i>
															<span class="m-nav__link-text">
																Messages
															</span>
														</a>
													</li>
													<li class="m-nav__item">
														<a href="" class="m-nav__link">
															<i class="m-nav__link-icon flaticon-info"></i>
															<span class="m-nav__link-text">
																FAQ
															</span>
														</a>
													</li>
													<li class="m-nav__item">
														<a href="" class="m-nav__link">
															<i class="m-nav__link-icon flaticon-lifebuoy"></i>
															<span class="m-nav__link-text">
																Support
															</span>
														</a>
													</li>
													<li class="m-nav__separator m-nav__separator--fit"></li>
													<li class="m-nav__item">
														<a href="#"
															class="btn btn-outline-danger m-btn m-btn--pill m-btn--wide btn-sm">
															Submit
														</a>
													</li>
												</ul>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
				<!-- END: Subheader -->
				<div class="m-content">
					<div class="row">
						<div class="col-md-12">
							<!--begin::Portlet-->
							<div class="m-portlet m-portlet--tab">
								<div class="m-portlet__head">
									<div class="m-portlet__head-caption">
										<div class="m-portlet__head-title">
											<span class="m-portlet__head-icon m--hide">
												<i class="la la-gear"></i>
											</span>
											<h3 class="m-portlet__head-text">
												Register Amoeba
											</h3>
										</div>
									</div>
								</div>
								<!--begin::Form-->
								<form class="m-form m-form--fit m-form--label-align-right"
									action="/<% if (editing) { %>edit-amoeba<% } else { %>register-amoeba<% } %>"
									method="POST">
									<div class="m-portlet__body">
										<div class="row">
											<div class="col-md-4">
												<div class="form-group m-form__group">
													<label for="idAmoeba">
														ID Amoeba
													</label>
													<input type="text" class="form-control m-input m-input--solid"
														name="idAmoeba" id="idAmoeba" placeholder="ID Amoeba"
														value="<% if (editing) { %><%= amoeba.idAmoeba %><% } %>">
												</div>
											</div>
											<div class="col-md-4">
												<div class="form-group m-form__group">
													<label for="namaAmoeba">
														Nama Amoeba
													</label>
													<input type="text" class="form-control m-input m-input--solid"
														name="namaAmoeba" id="namaAmoeba" placeholder="Nama Amoeba"
														value="<% if (editing) { %><%= amoeba.namaAmoeba %><% } %>">
												</div>
											</div>
											<div class="col-md-4">
												<div class="form-group m-form__group">
													<label for="batch">
														Batch
													</label>
													<input type="text" class="form-control m-input m-input--solid" name="batch"
														id="batch" placeholder="Batch"
														value="<% if (editing) { %><%= amoeba.batch %><% } %>">
												</div>
											</div>
										</div>
										<br>
										<div class="form-group m-form__group">
											<label for="deskripsi1">
												Deskripsi
											</label>
											<textarea class="form-control m-input" name="deskripsi1" id="deskripsi1"
												rows="3"><% if (editing) { %><%= amoeba.deskripsi1 %><% } %></textarea>
										</div>
										<div class="form-group m-form__group">
											<label for="deskripsi(FF)">
												Deskripsi (FF)
											</label>
											<textarea class="form-control m-input" name="deskripsi(FF)" id="deskripsi(FF)"
												rows="3"><% if (editing) { %><%= amoeba.deskripsi(FF) %><% } %></textarea>
										</div>							
										<br>
										<div class="row">
											<div class="col-md-6">
												<div class="form-group m-form__group">
													<label for="statusAmoeba">
														Status Amoeba
													</label>
													<select class="form-control m-input m-input--solid"
														name="statusAmoeba" id="statusAmoeba">
														<option>
															Aktif
														</option>
														<option>
															Tidak Aktif
														</option>
													</select>
												</div>
											</div>
											<div class="col-md-6">
												<div class="form-group m-form__group">
													<label for="currentPhase">
														Current Phase
													</label>
													<select class="form-control m-input m-input--solid"
														name="currentPhase" id="currentPhase"
														value="<% if (editing) { %><%= amoeba.currentPhase %><% } %>">
														<option>
															CV
														</option>
														<option>
															PV
														</option>
														<option>
															BMV
														</option>
														<option>
															MV
														</option>
													</select>
												</div>
											</div>
										</div>
										<br>
										<div class="row">
											<div class="col-md-6">
												<div class="form-group m-form__group">
													<label for="incbAcc">
														Incubation / Acceleration
													</label>
													<select class="form-control m-input m-input--solid" name="incbAcc"
														id="incbAcc">
														<option>
															Incubation
														</option>
														<option>
															Acceleration
														</option>
													</select>
												</div>
											</div>
											<div class="col-md-6">
												<div class="form-group m-form__group">
													<label for="tipeInovasi">
														Tipe Inovasi
													</label>
													<select class="form-control m-input m-input--solid"
														name="tipeInovasi" id="tipeInovasi"
														value="<% if (editing) { %><%= amoeba.tipeInovasi %><% } %>">
														<option>
															Digitalization
														</option>
														<option>
															Digitization
														</option>
													</select>
												</div>
											</div>
										</div>
										<br>
										<div class="form-group m-form__group">
											<label for="dtp">
												DTP
											</label>
											<textarea class="form-control m-input" name="dtp" id="dtp"
												rows="3"><% if (editing) { %><%= amoeba.dtp %><% } %></textarea>
										</div>
										<div class="row">
											<div class="col-md-4">
												<div class="form-group m-form__group">
													<label for="areaInovasi">
														5 Area Inovasi
													</label>
													<select class="form-control m-input m-input--solid"
														name="areaInovasi" id="areaInovasi"
														value="<% if (editing) { %><%= amoeba.areaInovasi %><% } %>">
														<option>
															-
														</option>
														<option>
															NP
														</option>
														<option>
															BO
														</option>
														<option>
															CX
														</option>
														<option>
															EX
														</option>
														<option>
															BM
														</option>
													</select>
												</div>
											</div>
											<div class="col-md-4">
												<div class="form-group m-form__group">
													<label for="mappingPilarDDS">
														Mapping Pilar DDS
													</label>
													<select class="form-control m-input m-input--solid"
														name="mappingPilarDDS" id="mappingPilarDDS"
														value="<% if (editing) { %><%= amoeba.mappingPilarDDS %><% } %>">
														<option>
															Indihome Product
														</option>
														<option>
															Financial Service
														</option>
														<option>
															Commerce
														</option>
														<option>
															Logistic
														</option>
														<option>
															Games
														</option>
														<option>
															Travel / Tourism
														</option>
														<option>
															Video / Music
														</option>
														<option>
															Smartcity
														</option>
														<option>
															GOV / BUMN
														</option>
														<option>
															Enterprise / SME
														</option>
														<option>
															Advertising
														</option>
														<option>
															IoT
														</option>
														<option>
															BigData Service
														</option>
														<option>
															Others
														</option>
													</select>
												</div>
											</div>
											<div class="col-md-4">
												<div class="form-group m-form__group">
													<label for="mappingCFU">
														Mapping CFU
													</label>
													<select class="form-control m-input m-input--solid"
														name="mappingCFU" id="mappingCFU"
														value="<% if (editing) { %><%= amoeba.mappingCFU %><% } %>">
														<option>
															ENT
														</option>
														<option>
															DITCONS
														</option>
														<option>
															DIGITAL
														</option>
														<option>
															WINS
														</option>
														<option>
															MOBILE
														</option>
													</select>
												</div>
											</div>
										</div>
										<br>
										<div class="row">
											<div class="col-md-6">
												<div class="form-group m-form__group">
													<label for="core">
														Core
													</label>
													<select class="form-control m-input m-input--solid" name="core"
														id="core" value="<% if (editing) { %><%= amoeba.core %><% } %>">
														<option>
															B2B
														</option>
														<option>
															B2C
														</option>
														<option>
															B2B & B2C
														</option>
													</select>
												</div>
											</div>
											<div class="col-md-6">
												<div class="form-group m-form__group">
													<label for="ecosystem">
														Ecosystem
													</label>
													<select class="form-control m-input m-input--solid" name="ecosystem"
														id="ecosystem"
														value="<% if (editing) { %><%= amoeba.ecosystem %><% } %>">
														<option>
															GOV/SME
														</option>
														<option>
															GOV/SME/LE
														</option>
														<option>
															GOV
														</option>
														<option>
															LE
														</option>
														<option>
															SME
														</option>
														<option>
															LE/SME
														</option>
													</select>
												</div>
											</div>
										</div>
									</div>
									<div class="m-portlet__foot m-portlet__foot--fit">
										<div class="m-form__actions">
											<button type="submit" class="btn btn-success">
												<% if (editing) { %>Update Amoeba<% } else { %> Submit <% } %>
											</button>
											<% if (editing) { %>
												<a href="/view-amoeba" class="btn btn-secondary"> Cancel </a>
											<% } else { %>
											<button type="reset" class="btn btn-secondary"> Cancel </button>
											<% } %>
										</div>
									</div>
								</form>
								<!--end::Form-->
							</div>
							<!--end::Portlet-->
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- end:: Body -->
		<!-- begin::Footer -->
		<%- include('includes/footer.ejs')%>
		<!-- end::Footer -->
	</div>
	<!-- end:: Page -->
	<!-- begin::Quick Sidebar -->
	<%- include('includes/quick-sidebar.ejs')%>
	<!-- end::Quick Sidebar -->
	<!-- begin::Scroll Top -->
	<div id="m_scroll_top" class="m-scroll-top">
		<i class="la la-arrow-up"></i>
	</div>
	<!-- end::Scroll Top -->
	<!-- begin::Quick Nav -->
	<%- include('includes/quick-nav.ejs')%>
	<!-- begin::Quick Nav -->
	<!--begin::Base Scripts -->
	<%- include('includes/base-scripts.ejs')%>
	<!--end::Base Scripts -->
	<!--begin::Page Snippets -->
	<script src="assets/app/js/dashboard.js" type="text/javascript"></script>
	<!--end::Page Snippets -->
</body>
<!-- end::Body -->

</html>