				<div class="m-content">
					<div class="row">
						<div class="col-md-12">
							<!--begin::Portlet-->
							<div class="m-portlet m-portlet--tab">
								<div class="m-portlet__head">
									<div class="m-portlet__head-caption">
										<div class="m-portlet__head-title">
											<span class="m-portlet__head-icon m--hide">
												<i class="la la-gear"></i>
											</span>
											<h3 class="m-portlet__head-text">
												Add Amoeba
											</h3>
										</div>
									</div>
								</div>
								<!--begin::Form-->
								<form class="m-form m-form--fit m-form--label-align-right" action="/amoeba/add-amoeba"
									method="POST">
									<div class="m-portlet__body">
										<div class="m-form__content">
											<div class="m-alert m-alert--icon alert alert-danger m--hide" role="alert"
												id="m_form_1_msg">
												<div class="m-alert__icon">
													<i class="la la-warning"></i>
												</div>
												<div class="m-alert__text">
													Oh snap! Change a few things up and try submitting again.
												</div>
												<div class="m-alert__close">
													<button type="button" class="close" data-close="alert"
														aria-label="Close"></button>
												</div>
											</div>
											<div class="row">
												<div class="col-md-6">
													<div class="form-group m-form__group hidden">
														<label for="NamaAmoeba">
															Nama Amoeba
														</label>
														<input type="text"
															class="form-control m-input m-input--solid disabled"
															name="NamaAmoeba" id="NamaAmoeba" placeholder="Nama Amoeba" value="">
													</div>
												</div>
												<div class="col-md-6">
													<div class="form-group m-form__group">
														<label for="BatchAmoeba">
															Batch Amoeba
														</label>
														<input type="text" class="form-control m-input m-input--solid"
															name="BatchAmoeba" id="BatchAmoeba" placeholder="Batch-xx"
															value="">
													</div>
												</div>
											</div>
											<br>
											<div class="row">
												<div class="col-md-6">
													<div class="form-group m-form__group">
														<label for="AreaInovasi">
															AreaInovasi
														</label>
														<select class="form-control m-input m-input--solid" name="AreaInovasi"
															id="AreaInovasi">
															<option value="1">
																-
															</option>
															<option value="2">
																BO
															</option>
															<option value="3">
																CX
															</option>
															<option value="4">
																EX
															</option>
															<option value="5">
																NP
															</option>
															<option value="6">
																BM
															</option>
														</select>
													</div>
												</div>
												<div class="col-md-6">
													<div class="form-group m-form__group">
														<label for="TipeInovasi">
															Tipe Inovasi
														</label>
														<select class="form-control m-input m-input--solid" name="TipeInovasi"
															id="TipeInovasi">
															<option value="Digitization">
																Digitization
															</option>
															<option value="Digitalization">
																Digitalization
															</option>
														</select>
													</div>
												</div>
											</div>
											<br>
											<div class="row">
												<div class="col-md-6">
													<div class="form-group m-form__group">
														<label for="StatusAmoeba">
															Status Amoeba
														</label>
														<select class="form-control m-input m-input--solid" name="StatusAmoeba"
															id="StatusAmoeba">
															<option value="0">
																Inactive
															</option>
															<option value="1">
																Active
															</option>
														</select>
													</div>
												</div>
												<div class="col-md-6">
													<div class="form-group m-form__group">
														<label for="IncbAcc">
															Incubation / Acceleration
														</label>
														<select class="form-control m-input m-input--solid" name="IncbAcc"
															id="IncbAcc">
															<option value="Incubation">
																Incubation
															</option>
															<option value="Acceleration">
																Acceleration
															</option>
														</select>
													</div>
												</div>
											</div>
											<br>
											<div class="row">
												<div class="col-md-6">
													<div class="form-group m-form__group">
														<label for="Tribe">
															Tribe
														</label>
														<select class="form-control m-input m-input--solid" name="Tribe"
															id="Tribe">
															<option value="1">
																Tribe Enterprise & Government Digitization
															</option>
															<option value="2">
																Tribe Communication
															</option>
															<option value="3">
																Tribe BUMN
															</option>
															<option value="4">
																Tribe Travel & Tourism
															</option>
															<option value="5">
																Tribe Logistic & Agriculture
															</option>
															<option value="6">
																Tribe Employe Experience Digitization
															</option>
															<option value="7">
																Tribe Internet of Things
															</option>
															<option value="8">
																Tribe Smart City
															</option>
															<option value="9">
																Tribe Digital Infrastructure & Cyber Security
															</option>
															<option value="10">
																Tribe Indihome & SMB Digitization
															</option>
															<option value="11">
																Tribe Indihome Digitalization
															</option>
															<option value="12">
																Tribe Finserv
															</option>
															<option value="13">
																Tribe Video
															</option>
															<option value="14">
																Tribe Ads
															</option>
															<option value="15">
																Tribe Music
															</option>
															<option value="16">
																Tribe Game
															</option>
															<option value="17">
																Tribe E-Commerce
															</option>
															<option value="18">
																Tribe CODEX
															</option>
															<option value="19">
																Tribe Smart Platform
															</option>
															<option value="20">
																Tribe SMB Digitalization
															</option>
															<option value="21">
																Tribe Enterprise Digitalization
															</option>
															<option value="22">
																Tribe Government Digitalization
															</option>
															<option value="23">
																Tribe Indonesia Digital Learning
															</option>
															<option value="24">
																Big Data Management
															</option>
															<option value="25">
																Big Data Platform
															</option>
															<option value="26">
																Digital Talent Sourcing, Service, Performance, Dev
															</option>
														</select>
													</div>
												</div>
											</div>
											<br>
											<div class="row">
												<div class="col-md-6">
													<div class="form-group m-form__group">
														<label for="Deskripsi">
															Deskripsi
														</label>
														<textarea class="form-control m-input m-input--solid disabled" name="Deskripsi" id="Deskripsi" placeholder="Deskripsi"></textarea>
													</div>
												</div>
												<div class="col-md-6">
													<div class="form-group m-form__group">
														<label for="DeskripsiFF">
															Deskripsi FF
														</label>
														<textarea class="form-control m-input m-input--solid disabled" name="DeskripsiFF" id="DeskripsiFF" placeholder="Deskripsi"></textarea>
													</div>
												</div>
											</div>
