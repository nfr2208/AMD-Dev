											<hr>
											<h1>DTP</h1>
											<div class="row">
												<div class="col-md-6">
													<div class="form-group m-form__group hidden">
														<label for="Facebook">
															Facebook
														</label>
														<input type="text"
															class="form-control m-input m-input--solid disabled"
															name="Facebook" id="Facebook" placeholder="Facebook Amoeba" value="">
													</div>
												</div>
												<div class="col-md-6">
													<div class="form-group m-form__group">
														<label for="Twitter">
															Twitter
														</label>
														<input type="text" class="form-control m-input m-input--solid"
															name="Twitter" id="Twitter" placeholder="Twitter Amoeba"
															value="">
													</div>
												</div>
											</div>
											<br>
											<div class="row">
												<div class="col-md-6">
													<div class="form-group m-form__group hidden">
														<label for="Instagram">
																Instagram
														</label>
														<input type="text"
															class="form-control m-input m-input--solid disabled"
															name="Instagram" id="Instagram" placeholder="Instagram Amoeba" value="">
													</div>
												</div>
												<div class="col-md-6">
													<div class="form-group m-form__group">
														<label for="LinkedIn">
																LinkedIn
														</label>
														<input type="text" class="form-control m-input m-input--solid"
															name="LinkedIn" id="LinkedIn" placeholder="LinkedIn Amoeba"
															value="">
													</div>
												</div>
											</div>
											<br>
											<div class="row">
												<div class="col-md-6">
													<div class="form-group m-form__group hidden">
														<label for="Website">
															Website
														</label>
														<input type="text"
															class="form-control m-input m-input--solid disabled"
															name="Website" id="Website" placeholder="Website Amoeba" value="">
													</div>
												</div>
												<div class="col-md-6">
													<div class="form-group m-form__group">
														<label for="Youtube">
															Youtube
														</label>
														<input type="text" class="form-control m-input m-input--solid"
															name="Youtube" id="Youtube" placeholder="Youtube Amoeba"
															value="">
													</div>
												</div>
											</div>
											<br>
											<div class="row">
												<div class="col-md-12">
													<div class="form-group m-form__group hidden">
														<label for="Other">
															Other
														</label>
														<textarea class="form-control m-input m-input--solid disabled" name="Other" id="Other" placeholder="Other" rows="5"></textarea>
													</div>
												</div>
											</div>
										</div>
										<br>
										<div class="m-portlet__foot m-portlet__foot--fit">
											<div class="m-form__actions">
												<input type="hidden" value="<%= csrfToken %>" name="_csrf"
													id="csrfToken">
												<button type="submit" class="btn btn-success">
													Submit
												</button>
												<button type="reset" class="btn btn-secondary"> Cancel </button>
											</div>
										</div>
									</div>
								</form>
								<!--end::Form-->
							</div>
							<!--end::Portlet-->
						</div>
					</div>
				</div>
