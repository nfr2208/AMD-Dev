<%- include('../includes/head.ejs')%>
<link href="/assets/vendors/custom/fullcalendar/fullcalendar.bundle.css" rel="stylesheet" type="text/css" />
</head>
<!-- end::Head -->
<!-- end::Body -->

<body
	class="m-page--fluid m--skin- m-content--skin-light2 m-header--fixed m-header--fixed-mobile m-aside-left--enabled m-aside-left--skin-light m-aside-left--fixed m-aside-left--offcanvas m-aside-left--minimize m-brand--minimize m-footer--push m-aside--offcanvas-default">
	<!-- begin:: Page -->
	<div class="m-grid m-grid--hor m-grid--root m-page">
		<!-- BEGIN: Header -->
		<%- include('../includes/header.ejs')%>
		<!-- END: Header -->
		<!-- begin::Body -->
		<div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-body">
			<!-- BEGIN: Left Aside -->
			<%- include('../includes/left-aside.ejs')%>
			<!-- END: Left Aside -->
			<div class="m-grid__item m-grid__item--fluid m-wrapper">
				<!-- BEGIN: Subheader -->
				<div class="m-subheader ">
					<div class="d-flex align-items-center">
						<div class="mr-auto">
							<h3 class="m-subheader__title m-subheader__title--separator">
								Dashboard
							</h3>
							<ul class="m-subheader__breadcrumbs m-nav m-nav--inline">
								<li class="m-nav__item m-nav__item--home">
									<a href="#" class="m-nav__link m-nav__link--icon">
										<i class="m-nav__link-icon la la-home"></i>
									</a>
								</li>
								<li class="m-nav__separator">
									>
								</li>
								<li class="m-nav__item">
									<a href="" class="m-nav__link">
										<span class="m-nav__link-text">
											Dashboard
										</span>
									</a>
								</li>
							</ul>
						</div>
						<!-- <div>
							<span class="m-subheader__daterange" id="m_dashboard_daterangepicker">
								<span class="m-subheader__daterange-label">
									<span class="m-subheader__daterange-title"></span>
									<span class="m-subheader__daterange-date m--font-brand"></span>
								</span>
								<a href="#"
									class="btn btn-sm btn-brand m-btn m-btn--icon m-btn--icon-only m-btn--custom m-btn--pill">
									<i class="la la-angle-down"></i>
								</a>
							</span>
						</div> -->
					</div>
				</div>
				<!-- END: Subheader -->
				<div class="m-content">				
					<!--Begin::Section-->
					<div class="m-portlet">
						<div class="m-portlet__body  m-portlet__body--no-padding">
							<div class="row m-row--no-padding m-row--col-separator-xl">
								<div class="col-xl-4">
									<div class="m-widget1">
										<div class="m-widget1__item">
											<div class="row m-row--no-padding align-items-center">
												<div class="col">
													<h3 class="m-widget1__title">
														100% Inovator
													</h3>
													<span class="m-widget1__desc">
														Total of 100% Inovator
													</span>
												</div>
												<div class="col m--align-right">
													<span class="m-widget1__number m--font-brand">
														<p id="fullTeamData"></p><b><%= fullTeamData %></b>
													</span>
												</div>
											</div>
										</div>
										<div class="m-widget1__item">
											<div class="row m-row--no-padding align-items-center">
												<div class="col">
													<h3 class="m-widget1__title">
														40/60
													</h3>
													<span class="m-widget1__desc">
														Total of 40/60
													</span>
												</div>
												<div class="col m--align-right">
													<span class="m-widget1__number m--font-danger">
														<p id="fsTeamData"></p><b><%= fsTeamData %></b>
													</span>
												</div>
											</div>
										</div>
										<div class="m-widget1__item">
											<div class="row m-row--no-padding align-items-center">
												<div class="col">
													<h3 class="m-widget1__title">
														On Bootcamp
													</h3>
													<span class="m-widget1__desc">
														Total of On Bootcamp
													</span>
												</div>
												<div class="col m--align-right">
													<span class="m-widget1__number m--font-success">
														<p id="onBootcampTeamData"></p><b><%= onBootcampTeamData %></b>
													</span>
												</div>
											</div>
										</div>
										<div class="m-widget1__item">
											<div class="row m-row--no-padding align-items-center">
												<div class="col">
													<h3 class="m-widget1__title">
														Not Join
													</h3>
													<span class="m-widget1__desc">
														Total of Not Join
													</span>
												</div>
												<div class="col m--align-right">
													<span class="m-widget1__number m--font-accent">
														<p id="notJoinTeamData"></p><b><%= notJoinTeamData %></b>
													</span>
												</div>
											</div>
										</div>
									</div>
								</div>
								<div class="col-xl-4">
									<div class="m-widget14">
										<div class="m-widget14__header">
											<h3 class="m-widget14__title">
												Team Data Flagging
											</h3>
											<span class="m-widget14__desc">
												Detail of Team Data Flagging
											</span>
										</div>
										<div class="row  align-items-center">
											<div class="col">
												<div id="flagging_chart" class="m-widget14__chart"
													style="height: 160px">
												</div>
											</div>
											<div class="col">
												<div class="m-widget14__legends">
													<div class="m-widget14__legend">
														<span class="m-widget14__legend-bullet m--bg-danger"></span>
														<span class="m-widget14__legend-text">
															<p hidden id="alumniTeamData" title="<%= alumniTeamData %>"></p><b><%= alumniTeamData %></b> Alumni Status
														</span>
													</div>
													<div class="m-widget14__legend">
														<span class="m-widget14__legend-bullet m--bg-success"></span>
														<span class="m-widget14__legend-text">
															<p hidden id="activeTalentTeamData" title="<%= activeTalentTeamData %>"></p><b><%= activeTalentTeamData %></b> Active Talent Status
														</span>
													</div>
													<div class="m-widget14__legend">
														<span class="m-widget14__legend-bullet m--bg-warning"></span>
														<span class="m-widget14__legend-text">
															<p hidden id="moveTeamData" title="<%= moveTeamData %>"></p><b><%= moveTeamData %></b> Move Team Status
														</span>
													</div>
												</div>
											</div>
										</div>
									</div>
								</div>
								<div class="col-xl-4">
									<div class="m-widget14">
										<div class="m-widget14__header">
											<h3 class="m-widget14__title">
												Team Data Status Chart
											</h3>
											<span class="m-widget14__desc">
												Chart of Active/Inactive Team Data
											</span>
										</div>
										<div class="row  align-items-center">
											<div class="col">
												<div id="status_chart" class="m-widget14__chart"
													style="height: 160px">
													<div class="m-widget14__stat">
														<p hidden id="totalTeamData" title="<%= totalTeamData %>"></p><b><%= totalTeamData %></b>
													</div>
												</div>
											</div>
											<div class="col">
												<div class="m-widget14__legends">
													<div class="m-widget14__legend">
														<span class="m-widget14__legend-bullet m--bg-success"></span>
														<span class="m-widget14__legend-text">
															<p hidden id="activeTeamData" title="<%= activeTeamData %>"></p><b><%= activeTeamData %></b> Active Status
														</span>
													</div>
													<div class="m-widget14__legend">
														<span class="m-widget14__legend-bullet m--bg-danger"></span>
														<span class="m-widget14__legend-text">
															<p hidden id="notActiveTeamData" title="<%= notActiveTeamData %>"></p><b><%= notActiveTeamData %></b> Not Active Status
														</span>
													</div>
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
					<!--End::Section-->
				</div>
			</div>
		</div>
		<!-- end:: Body -->
		<!-- begin::Footer -->
		<%- include('../includes/footer.ejs')%>
		<!-- end::Footer -->
	</div>
	<!-- end:: Page -->
	<!-- begin::Quick Sidebar -->
	<%- include('../includes/quick-sidebar.ejs')%>
	<!-- end::Quick Sidebar -->
	<!-- begin::Scroll Top -->
	<div id="m_scroll_top" class="m-scroll-top">
		<i class="la la-arrow-up"></i>
	</div>
	<!-- end::Scroll Top -->
	<!--begin::Base Scripts -->
	<%- include('../includes/base-scripts.ejs')%>
	<!--end::Base Scripts -->
	<!--begin::Page Vendors -->
	<script src="/assets/vendors/custom/fullcalendar/fullcalendar.bundle.js" type="text/javascript"></script>
	<!--end::Page Vendors -->
	<!--begin::Page Snippets -->
	<script src="/assets/app/js/chart-teamdata.js" type="text/javascript"></script>
	<!--end::Page Snippets -->
</body>
<!-- end::Body -->

</html>