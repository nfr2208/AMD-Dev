														<option value="3">
															TR6
														</option>
														<option value="4">
															TR7
														</option>
														<option value="5">
															METRANET
														</option>
														<option value="6">
															DSO
														</option>
														<option value="7">
															DDS
														</option>
														<option value="8">
															HCM
														</option>
														<option value="9">
															DIT/KEU
														</option>
														<option value="10">
															INFOMEDIA
														</option>
														<option value="11">
															TELIN
														</option>
														<option value="12">
															TELKOM TELSTRA
														</option>
														<option value="13">
															SIGMA
